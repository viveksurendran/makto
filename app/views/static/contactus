<script>
function initMap() {
   var uluru = {lat: 11.031670,  lng:77.018820 };
   var map = new google.maps.Map(document.getElementById('map'), {
     zoom: 15,
     center: uluru
   });
   var marker = new google.maps.Marker({
     position: uluru,
     map: map
   });
 }
</script>
